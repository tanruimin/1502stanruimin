<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="180.0" android:centerColor="#ffffffff" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ff1f86ed" android:endColor="#ffa6d3ff" android:useLevel="true" android:angle="180.0" android:type="linear" />
            </shape>
        </clip>
    </item>
</layer-list>