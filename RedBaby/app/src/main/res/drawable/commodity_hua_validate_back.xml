<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ffe8e8e8" android:endColor="#ffe8e8e8" android:angle="270.0" android:centerY="0.5" android:centerColor="#ffe8e8e8" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ff75c72b" android:endColor="#ff75c72b" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff75c72b" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ff75c72b" android:endColor="#ff75c72b" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff75c72b" />
            </shape>
        </clip>
    </item>
</layer-list>