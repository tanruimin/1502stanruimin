<LinearLayout android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout
        android:id="@id/rl_my_orders"
        android:background="@color/pub_color_one"
        android:paddingRight="@dimen/android_public_space_14dp"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/android_public_space_40dp">
        <ImageView
            android:id="@id/icon_orders"
            android:visibility="gone"
            android:layout_width="@dimen/android_public_space_25dp"
            android:layout_height="@dimen/android_public_space_25dp"
            android:src="@drawable/icon_my_order"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true" />
        <TextView
            android:textSize="@dimen/android_public_textsize_15sp"
            android:textColor="@color/pub_color_fifteen"
            android:gravity="center_vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/act_myebuy_myorders"
            android:layout_toRightOf="@id/icon_orders"
            android:layout_centerVertical="true"
            android:layout_marginLeft="8dp"/>
        <ImageView
            android:id="@id/img_order_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/arrow_next"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:contentDescription="@string/app_name" />
        <TextView
            android:textSize="@dimen/android_public_textsize_13sp"
            android:textColor="@color/act_myebuy_color1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="5.0dip"
            android:text="@string/act_myebuy_myorders_notice"
            android:layout_toLeftOf="@id/img_order_arrow"
            android:layout_centerVertical="true" />
    </RelativeLayout>
    <View
        android:id="@id/view_divider_modeule"
        android:background="@color/pub_color_six"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/pub_margin_or_padding_space_half_of_one" />
    <LinearLayout
        android:orientation="horizontal"
        android:background="@color/pub_color_one"
        android:layout_width="fill_parent"
        android:layout_height="77.0dip">
        <RelativeLayout
            android:id="@id/rl_orders_waitfor_pay"
            android:padding="@dimen/ios_public_space_14px"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true">
                <ImageView
                    android:id="@id/img_icon_act_myebuy_waitfor_pay"
                    android:layout_width="@dimen/android_public_space_25dp"
                    android:layout_height="@dimen/android_public_space_25dp"
                    android:src="@drawable/icon_act_myebuy_waitfor_pay" />
                <TextView
                    android:textSize="@dimen/android_public_textsize_13sp"
                    android:textColor="@color/pub_color_fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/android_public_space_4dp"
                    android:text="@string/wait_for_pay"
                    android:layout_centerInParent="true" />
            </LinearLayout>
            <TextView
                android:textSize="@dimen/android_public_textsize_10sp"
                android:textStyle="bold"
                android:textColor="@color/pub_color_one"
                android:gravity="center"
                android:id="@id/tv_orders_waitforpay_num"
                android:background="@drawable/myebuy_orders_center_num_bg"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/android_public_space_6dp"
                android:layout_marginRight="@dimen/android_public_space_15dp"
                android:minWidth="@dimen/android_space_30px"
                android:text="75"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true" />
        </RelativeLayout>
        <RelativeLayout android:id="@id/rl_orders_waitfor_accept"
            android:padding="@dimen/ios_public_space_14px"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">
            <LinearLayout android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true">
                <ImageView
                    android:id="@id/img_icon_act_myebuy_waitfor_accept"
                    android:layout_width="@dimen/android_public_space_25dp"
                    android:layout_height="@dimen/android_public_space_25dp"
                    android:src="@drawable/icon_act_myebuy_waitfor_accept" />
                <TextView
                    android:textSize="@dimen/android_public_textsize_13sp"
                    android:textColor="@color/pub_color_fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/android_public_space_4dp"
                    android:text="@string/wait_for_receipt"
                    android:layout_centerInParent="true" />
            </LinearLayout>
            <TextView
                android:textSize="@dimen/android_public_textsize_10sp"
                android:textStyle="bold"
                android:textColor="@color/pub_color_one"
                android:gravity="center"
                android:id="@id/tv_orders_waitforaccept_num"
                android:background="@drawable/myebuy_orders_center_num_bg"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/android_public_space_6dp"
                android:layout_marginRight="@dimen/android_public_space_15px"
                android:minWidth="@dimen/android_space_30px"
                android:text="1"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@id/rl_orders_waitfor_evaluate"
            android:padding="@dimen/ios_public_space_14px"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true">
                <ImageView
                    android:id="@id/img_icon_act_myebuy_waitfor_evaluation"
                    android:layout_width="@dimen/android_public_space_25dp"
                    android:layout_height="@dimen/android_public_space_25dp"
                    android:src="@drawable/icon_act_myebuy_waitfor_evaluation" />
                <TextView
                    android:textSize="@dimen/android_public_textsize_13sp"
                    android:textColor="@color/pub_color_fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/android_public_space_4dp"
                    android:text="@string/wait_for_evaluation"
                    android:layout_centerInParent="true" />
            </LinearLayout>
            <TextView
                android:textSize="@dimen/android_public_textsize_10sp"
                android:textStyle="bold"
                android:textColor="@color/pub_color_one"
                android:gravity="center"
                android:id="@id/tv_orders_waitforevaluate_num"
                android:background="@drawable/myebuy_orders_center_num_bg"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/android_public_space_6dp"
                android:layout_marginRight="@dimen/android_public_space_15px"
                android:minWidth="@dimen/android_space_30px"
                android:text="1"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@id/rl_orders_waitfor_return"
            android:padding="@dimen/ios_public_space_14px"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">
            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true">
                <ImageView
                    android:id="@id/img_icon_act_myebuy_waitfor_return"
                    android:layout_width="@dimen/android_public_space_25dp"
                    android:layout_height="@dimen/android_public_space_25dp"
                    android:src="@drawable/icon_act_myebuy_waitfor_return" />
                <TextView
                    android:textSize="@dimen/android_public_textsize_13sp"
                    android:textColor="@color/pub_color_fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/android_public_space_4dp"
                    android:text="@string/returning"
                    android:layout_centerInParent="true" />
            </LinearLayout>
            <TextView
                android:textSize="@dimen/android_public_textsize_10sp"
                android:textStyle="bold"
                android:textColor="@color/pub_color_one"
                android:gravity="center"
                android:id="@id/tv_orders_waitforreturn_num"
                android:background="@drawable/myebuy_orders_center_num_bg"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/android_public_space_6dp"
                android:layout_marginRight="@dimen/android_public_space_15px"
                android:minWidth="@dimen/android_space_30px"
                android:text="1"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true" />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>